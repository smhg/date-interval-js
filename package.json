{
  "name": "@smhg/date-interval",
  "version": "0.2.2",
  "description": "Algebraic operations for time intervals",
  "main": "dist/interval.js",
  "scripts": {
    "lint": "eslint src test",
    "clean": "rm -rf dist",
    "build": "npm run lint && babel --source-maps -d dist/ src/",
    "test": "npm run build && mocha --compilers js:babel-register test",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags",
    "prepublish": "npm run test",
    "postpublish": "npm run clean"
  },
  "keywords": [
    "time",
    "interval",
    "iso",
    "8601",
    "iso 8601",
    "iso8601",
    "time interval",
    "timespan"
  ],
  "author": "Sam Hauglustaine",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.4.5",
    "babel-eslint": "4.1.6",
    "babel-preset-es2015": "6.3.13",
    "babel-register": "6.4.3",
    "eslint": "1.10.3",
    "mocha": "2.3.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smhg/date-interval-js.git"
  },
  "homepage": "https://github.com/smhg/date-interval-js#readme",
  "bugs": {
    "url": "https://github.com/smhg/date-interval-js/issues"
  },
  "dependencies": {
    "date-duration": "0.3.0"
  }
}
