{
  "name": "@smhg/date-interval",
  "version": "1.0.0",
  "description": "Algebraic operations for time intervals",
  "main": "dist/interval.js",
  "scripts": {
    "lint": "eslint src test",
    "clean": "rm -rf dist",
    "build": "npm run lint && babel --source-maps -d dist/ src/",
    "test": "npm run build && mocha --compilers js:babel-register test",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run test",
    "postpublish": "npm run clean"
  },
  "keywords": [
    "time",
    "interval",
    "iso",
    "8601",
    "iso 8601",
    "iso8601",
    "time interval",
    "timespan"
  ],
  "author": "Sam Hauglustaine",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "mocha": "^3.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smhg/date-interval-js.git"
  },
  "homepage": "https://github.com/smhg/date-interval-js#readme",
  "bugs": {
    "url": "https://github.com/smhg/date-interval-js/issues"
  },
  "dependencies": {
    "date-duration": "^1.1.0"
  }
}
